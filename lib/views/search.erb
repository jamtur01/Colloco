<br />

<h1>Search results</h1>

<br />

<table class="table table-striped table-bordered">
  <tr>
    <td class="mt">ID</td>
    <td class="mt">Title</td>
    <td class="mt">Maker</td>
    <td class="mt">Date</td>
    <td class="mt">Source</td>
    <td class="mt">Size</td>
    <td class="mt">Notes</td>
    <td class="mt">Price</td>
  </tr>
  <% if @results %>
        <%@results.each do |map|%>
          <tr>
            <td><a href=/map/<%= map["id"] %>><%= map["id"] %></a></td>
            <td><%= map["title"] %></td>
            <td><%= map["maker"] %></td>
            <td><%= map["date"] %></td>
            <td><%= map["source"] %></td>
            <td><%= map["size"] %></td>
            <td><%= map["notes"] %></td>
            <td><%= map["price"] %></td>
          </tr>
        <% end %>
   <% end %>
</table>
